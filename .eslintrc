{
    "env": {
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "extends": "eslint:recommended",
    "root": true,
    "globals": {},
    "plugins":[
    ],
    "rules": {
        "indent": [
            "error",
            4
        ],
        "no-trailing-spaces": "error",
        "no-console": "off",
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}